<%= form_with(model: @warehouse) do |form| %>
  <div>
		<%= form.label :name%>
		<%= form.text_field :name, required: true %>
	</div>
	<div>
		<%= form.label :description %>
  	<%= form.text_field :description, required: true %>
	</div>
	<div>
		<%= form.label :code %>
  	<%= form.text_field :code, maxlength:3 %>
	</div>
	<div>
		<%= form.label :address %>
  	<%= form.text_field :address, required: true %>
	</div>
	<div>
  	<%= form.label :city %>
  	<%= form.text_field :city, required: true %>
	</div>
	<div>
  	<%= form.label :cep %>
  	<%= form.text_field :cep, maxlength:9, placeholder:"00000-000" %>
	</div>
  <div>
  	<%= form.label :area %>
  	<%= form.number_field :area, required: true %>
	</div>
  
  <div> 
		<%= form.submit 'Enviar' %>
	</div>

<% end %>